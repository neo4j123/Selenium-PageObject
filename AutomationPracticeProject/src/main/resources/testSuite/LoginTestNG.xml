<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<listeners>
		<listener class-name="helper.listener.RetryListener"></listener>
		<listener class-name="helper.listener.ExtentListener"></listener>
	</listeners>
	<test name="Login with valid username and password">
		<classes>
			<class name="testScripts.MyAccount.Login.TC_001" />
		</classes>
	</test> <!-- Test -->
	<test name="Login with incorrect username and password">
		<classes>
			<class name="testScripts.MyAccount.Login.TC_002" />
		</classes>
	</test> <!-- Test -->
	<test name="Login with correct username and blank password">
		<classes>
			<class name="testScripts.MyAccount.Login.TC_003" />
		</classes>
	</test> <!-- Test -->
	<test name="Login with blank username and valid password">
		<classes>
			<class name="testScripts.MyAccount.Login.TC_004" />
		</classes>
	</test> <!-- Test -->
	<test name="Login with blank username and password">
		<classes>
			<class name="testScripts.MyAccount.Login.TC_005" />
		</classes>
	</test> <!-- Test -->	
</suite> <!-- Suite -->